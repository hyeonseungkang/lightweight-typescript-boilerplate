{
  "$schema": "https://swc.rs/schema.json",
  "sourceMaps": true,
  "isModule": true,
  "jsc": {
    "target": "ES2023",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "baseUrl": "./"
  },
  "minify": false,
  "exclude": [
    "node_modules",
    "dist"
  ]
}
